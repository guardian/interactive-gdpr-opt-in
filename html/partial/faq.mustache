<details class="oi-faq">
	<summary class="oi-faq__header">
		<figure irrelevant>
			{{> svg-plus}}
			{{> svg-minus}}
		</figure>
		{{title}}
	</summary>
	<div class="oi-faq__details">
		{{#body}}
			<p>{{.}}</p>
		{{/body}}
		<footer data-name="{{title}}" class="oi-faq__footer"></footer>
	</div>
</details>
